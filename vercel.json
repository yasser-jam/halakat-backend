{
    "version": 2,
    "builds": [{
        "src": "src/main.ts",
        "use": "@vercel/node"
    }],
    "buildCommand": "npm run build",
    "installCommand": "npm install --force",
    "routes": [{
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }]
}